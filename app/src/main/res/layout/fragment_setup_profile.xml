<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feature_profile.presentation.fragments.SetupProfileFragment">


    <include
        android:id="@+id/layout_setup_profile"
        layout="@layout/layout_setup_profile_top" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_save_profile"
        android:layout_width="0dp"
        android:layout_height="@dimen/btnNormal"
        android:layout_marginHorizontal="@dimen/dpXXXL"
        android:layout_marginTop="@dimen/dpMedium"
        android:text="Save"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/layout_setup_profile" />

</androidx.constraintlayout.widget.ConstraintLayout>